<?xml version="1.0" encoding="UTF-8"?>
<aiml version="2.0">

<category>
	<pattern>^ si ^</pattern>
	<template>
		<think><set name="Afirmacion">SI</set></think>
		<srai><star/> <star index= "2"/></srai>
	</template>
</category>

<category>
	<pattern>$modo aprendizaje *</pattern>
	<template>Tomo nota
		<learnf>
			<category>
				<pattern>PATRONAPRENDIDO <eval><get name="UltimaPregunta"/></eval></pattern>
				<template><eval><star/></eval></template>
			</category>
		</learnf>
		<think><set name="PrimeraVez">unknown</set><set name="UltimaPregunta"><get name="PreguntaActual"/></set></think>
	</template>
</category>

<category>
	<pattern>$modo asociacion *</pattern>
	<template>Tomo nota
		<learnf>
			<category>
				<pattern>PATRONAPRENDIDO <eval><get name="UltimaPregunta"/></eval></pattern>
				<template><srai><eval><star/></eval></srai></template>
			</category>
		</learnf>
		<think><set name="PrimeraVez">unknown</set><set name="UltimaPregunta"><get name="PreguntaActual"/></set></think>
	</template>
</category>


<?xml version="1.0" encoding="UTF-8"?>
<aiml version="2.0">

	<category>
		<pattern>De que color tiene el pelo <set>compi</set></pattern>
		<template><star/> tiene el pelo <map name="pelo"><star/></map></template>
	</category>

	<category>
		<pattern>De que color tiene los ojos <set>compi</set></pattern>
		<template><star/> tiene los ojos de color <map name="ojos"><star/></map></template>
	</category>

	<category>
		<pattern>Que compa√±eros son rubios</pattern>
		<template></template>
	</category>

	
	<category>
		<pattern>Lista a tus <set>compis</set></pattern>
		<template>
			<think>
				<set var="actual">ruben</set>
				<set var="salida">pablo</set>
			</think>
			<condition var="actual">
				<li><value><star/></value><get var="salida"/></li>
				<li>
					<think>
						<set var="actual">
							<map name="compi_sucessor"><get var="actual"/></map>
						</set>
						<set var="salida"><get var="salida"/>, <get var="actual"/></set>
					</think>
					</loop>
				</li>
			</condition>
		</template>
	</category>
</aiml>
<?xml version="1.0" encoding="UTF-8"?>
<aiml version="2.0">

<category>
	<pattern>*</pattern>
	<template><srai>Unknown <star/></srai>

	</template>
</category>

<category>
	<pattern>* *</pattern>
	<template>
		<think>
			<set var="Fin"><star/></set>
			<get name="Lugar"/> <get name="Tema"/><get name="Pregunta"/><star/> <star index= "2"/>
		</think>
		<condition name ="Tema">
		
		<!-- PAT-->
		
			<li value="PAT">
				<condition name ="Pregunta">
					<li value="QUE">El Plan de Acción Titorial es un programa de atención personalizada dirigido a los alumnos de nuevo ingreso.</li>
					<li><think><set var="Solucion">false</set></think></li>
				</condition>
			</li>
			
		<!-- Horarios-->
			
			<li value="HORARIO">
				<condition name ="Lugar">
					<li value="SECRETARIA">De lunes a viernes, de 9:00 a 14:00 h.</li>
					<li><think><set var="Solucion">false</set></think></li>
				</condition>
			</li>
			
		<!-- Otro-->
			
			<li><think><set var="Solucion">false</set></think></li>
		</condition>
		
		<!-- Shifteo (Se elimina la primera palabra y se vuelve a probar)-->
		
		<condition var ="Solucion">
			<li value="false">
				<condition var ="Fin">
					<li value="Unknown">Soy un poco sensible.</li>
					<li><srai><star index= "2"/></srai></li>
				</condition>
			</li>
			<li></li>
		</condition>
		
		<!-- Reset-->
		<think>
			<set name="Lugar">Unknown</set>
			<set name="Tema">Unknown</set>
			<set name="Pregunta">Unknown</set>
		</think>
	</template>
</category>


		<!-- Segmentacion-->


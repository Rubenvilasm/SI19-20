<?xml version="1.0" encoding="UTF-8"?>
<aiml version="2.0">

<category>
	<pattern>*</pattern>
	<template><srai><star/> UnknownFinal</srai></template>
</category>

<category>
	<pattern>* *</pattern>
	<template>
		<think>
			<set var="Fin"><star index= "2"/></set>
			Lugar: <get name="Lugar"/> Tema: <get name="Tema"/> Pregunta: <get name="Pregunta"/> VerboEspecifico: <get name="VerboEspecifico"/> TemaEspecifico:<get name="TemaEspecifico"/> Primera: <star/> Segundo: <star index= "2"/>
		</think>
		<condition name ="Tema">
		
		<!-- PAT-->
		
			<li value="PAT">
				<condition name ="Pregunta">
					<li value="QUE">El Plan de Acción Titorial es un programa de atención personalizada dirigido a los alumnos de nuevo ingreso.</li>
					<li><think><set var="Solucion">false</set></think></li>
				</condition>
			</li>
			
		<!-- Horarios-->
			
			<li value="HORARIO">
				<condition name ="Lugar">
					<li value="SECRETARIA">De lunes a viernes, de 9:00 a 14:00 h.</li>
					<li><think><set var="Solucion">false</set></think></li>
				</condition>
			</li>

			<!-- Ubicacion-->
		
			<li value="UBICACION">
				<condition name ="Lugar">
							<li value="SECRETARIA">En la planta baja del edificio politécnico. Puedes preguntar en consejería para mayor exactitud.</li>
							<li value="ESEI">La ESEI está en el Edificio Politécnico s/n, 32004, Ourense.</li>
							<li><think><set var="Solucion">false</set></think></li>	
				</condition>
			</li>

			<!-- Estudios-->
		
			<li value="ESTUDIOS">
				<condition name ="Pregunta">
					<li value="QUE">
						<condition name ="Lugar">
							<li value="ESEI">Ingeniería informatica, entre otras cosas como el doble grado de ADE + EI o el máster en Ingenieria Informática.</li>
							<li><think><set var="Solucion">false</set></think></li>	
						</condition>					
					</li>
					<li value="POR QUE">
						<condition name ="Lugar">
							<li value="ESEI">Porque los estudios en ingenieria informatica están muy demandados en el mercado laboral.</li>
							<li><think><set var="Solucion">false</set></think></li>	
						</condition>
					</li>
					<li><think><set var="Solucion">false</set></think></li>	
				</condition>
			</li>
			
			<!-- Cafeteria-->
		
			<li value="CAFETERIA">
				<condition name ="Lugar">
							<li value="ESEI">Si, en la planta baja del edificio politécnico.</li>
							<li><think><set var="Solucion">false</set></think></li>	
				</condition>
			</li>	
			
			<!-- PIUNE-->
		
			<li value="PIUNE">Con el fin de facilitar la integración del alumnado con necesidades específicas de apoyo que participa en el PIUNE el centro creó un programa de aogida y detección</li>	
			
			
			<!-- Delegacion -->
		
			<li value="DELEGACION">
				<condition name ="Lugar">
							<li value="ESEI">Se la conoce como ESEIDA. Para contactar con ellos puedes ir a su local en la primera planta,o traves del telefono 988387000 o del email eseida@esei.uvigo.es</li>
							<li><think><set var="Solucion">false</set></think></li>	
				</condition>
			</li>
			
			<!--Edad-->
			<li value="EDAD">
				<condition name ="Pregunta">
					<li value="QUE">Tengo <bot name="age"/> años.</li>
					<li value="CUAL">Tengo <bot name="age"/> años.</li>
					<li value="CUANTOS">Tengo <bot name="age"/> años.</li>
					<li value="CUANDO">Cumplo años el <bot name="birthday"/>.</li>
					<li><think><set var="Solucion">false</set></think></li>
				</condition>
			</li>
			<!-- nombre-->
			<li value="NOMBRE">
				<condition name ="Pregunta">
					<li value="COMO">Me llamo <bot name="name"/>, un placer.</li>
					<li value="CUAL">Me llamo <bot name="name"/>, un placer.</li>
					<li value="QUIEN">Soy <bot name="name"/>, un placer.</li>
					<li><think><set var="Solucion">false</set></think></li>
				</condition>
			</li>

			<!-- Contactar-->
			<li value="CONTACTO">
				<condition name ="Pregunta">
					<li value="COMO">Puedes escribirme un correo a <bot name="email"/> o llamarme al <bot name="contacto"/>.</li>
					<li value="CUAL">Puedes escribirme un correo a <bot name="email"/> o llamarme al <bot name="contacto"/>.</li>
					<li><think><set var="Solucion">false</set></think></li>
				</condition>
			</li>

			<!--Genero-->
			<li value="GENERO">
				<condition name ="Pregunta">
					<li>Prefiero no hablar de mi genero ...</li>
				</condition>
			</li>

			<!--Botmasters-->
			<li value="BOTMASTER">
				<condition name ="Pregunta">
					<li value="QUIEN">Mis creadores son <bot name="botmasters"/>.</li>
					<li><think><set var="Solucion">false</set></think></li>
				</condition>
			</li>

			<!--Institucion-->
			<li value="ORGANIZACION">
				<condition name ="Pregunta">
					<li value="QUE">Nuestra organizacion es <bot name="organizacion"/>.</li>
					<li value="CUAL">Nuestra organizacion es <bot name="organizacion"/>.</li>
					<li><think><set var="Solucion">false</set></think></li>
				</condition>
			</li>

			<!--Version-->
			<li value="VERSION">
				<condition name ="Pregunta">
					<li value="QUE">Actualmente estamos en la version <bot name="version"/>.</li>
					<li value="CUAL">Actualmente estamos en la version <bot name="version"/>.</li>
					<li><think><set var="Solucion">false</set></think></li>
				</condition>
			</li>

			<!-- Trabajo -->
			
			<li value="JOB">
				<condition name ="Pregunta">
					<li value="QUE">En estos momentos soy <bot name="job"/>.</li>
					<li value="CUAL">En estos momentos soy <bot name="job"/>.</li>
					<li><think><set var="Solucion">false</set></think></li>
				</condition>
			</li>

			<!--Curso academico-->
			
			<li value="CURSO">
				<condition name ="Pregunta">
					<li value="QUE">Estoy en el <bot name="curso"/> año de <bot name="grado"/></li>
					<li value="CUAL">Estoy en el <bot name="curso"/> año de <bot name="grado"/></li>
					<li value="COMO">Estoy en el <bot name="curso"/> año de <bot name="grado"/></li>

					<li><think><set var="Solucion">false</set></think></li>
				</condition>
			</li>
			<!-- Hablar -->
		
			<li value="HABLAR">
				<condition name ="Lugar">
							<li value="UVIGO">Es una de la tres universidades de Galicia. Tiene 3 campus, en Ourense, en Vigo y en Pontevedra.</li>
							<li><think><set var="Solucion">false</set></think></li>	
				</condition>
			</li>                                     
			
			<!-- SET -->
		
			<li value="SET">
				<condition name ="Pregunta">
							<li value="QUE">Es el documento que otorga validez comunitaria al currículum académico.</li>
							<li><think><set var="Solucion">false</set></think></li>	
				</condition>
			</li>
			
			<!-- Practicas -->
		
			<li value="PRACTICAS">
				<condition name ="Pregunta">
							<li value="QUE">
							
								<condition name ="TemaEspecifico">
									<li value="INTERESES">La verdad es que no me interesa ninguna empresa en concreto. Hasta que no pruebe no sabre que me gusta.</li>
									<li>Puedes realizar prácticas curriculares o, si lo prefieres, extracurriculares.</li>	
								</condition>
							</li>
							<li value="DONDE">Esta todo actualizado en la web de la ESEI. Puedes acceder directamente a través de este link: https://esei.uvigo.es/docencia/practicas-en-empresa/</li>
							<li><think><set var="Solucion">false</set></think></li>	
				</condition>
			</li>    
			
			<!-- Recursos -->
		
			<li value="RECURSOS">
				<condition name ="Lugar">
							<li value="ESEI">La ESEI cuenta con diversas aulas, laboratorios, seminarios e despachos. Además los alumnos tendran un correo electrónico y acceso a la red Wireless de la universidad de Vigo</li>
							<li><think><set var="Solucion">false</set></think></li>	
				</condition>
			</li> 
		
			<!-- TFG -->
		
			<li value="TFG">
				<condition name ="TemaEspecifico">
							<li value="INTERESES">Me encataria hacer el TFG sobre una aplicacion educativa de Redes Neuronais Artificiais.</li>
							<li>Son los trabajos que los estudiantes deben realizar para obtener el grado de Ingenieria informatica o el MEI.</li>	
				</condition>
			</li> 
			
			<!-- Acceso -->
		
			<li value="ACCESO">
				<condition name ="Lugar">
							<li value="BIBLIOTECA">Puedes accceder a la sección de la biblioteca en la web de la ESEI o introduciendo "Biblioteca uVigo" en el buscador.</li>
							<li><think><set var="Solucion">false</set></think></li>	
				</condition>
			</li> 
			
						
			<!-- Evalucion -->
		
			<li value="EVALUCION">Con un justificante</li> 
			
						
			<!-- Deporte -->
		
			<li value="DEPORTE">
				<condition name ="Lugar">
							<li value="ESEI">No se</li>
							<li><think><set var="Solucion">false</set></think></li>	
				</condition>
			</li> 
			
			<!-- Asignatura -->
			
			<li value="ASIGNATURA">
				<condition name ="TemaEspecifico">
							<li value="MATRICULADO">
								<condition name ="Pregunta">
									<li value="QUE">
										<srai>GETLISTA <map name="listar">lista</map></srai>Estoy matriculado en <get name="Lista"/> y... Creo que no me olvido de ninguna.
									</li>
									<li value="CUANTO">
										<srai>GETLISTA <map name="listar">lista</map></srai>Estoy matriculado en <get name="Resultado"/> asignaturas.
									</li>
									<li>
										<think><set var="asignatura"><map name="asignatura"><star/></map></set></think>
										<condition var ="asignatura">
											<li value="ASIGNATURA">Si, <map name="asignaturaSinMarca"><star/></map> es una muy buena asignatura.</li>
											<li><think><set var="Solucion">false</set></think></li>	
										</condition>
									</li>	
								</condition>
							</li>
							<li><think><set var="Solucion">false</set></think></li>	
				</condition>
			</li>
			
		<!-- Otro-->
			
			<li>
				<condition name ="Pregunta">
				
				<!-- Donde-->
				
					<li value="DONDE">
						<condition name ="Lugar">
							<li value="SECRETARIA">Error?¿::En la planta baja del edificio politécnico. Puedes preguntar en consejería para mayor exactitud.</li>
							<li value="ESEI">wopssss La ESEI está en el Edificio Politécnico s/n, 32004, Ourense.</li>
							<li><think><set var="Solucion">false</set></think></li>	
						</condition>
					</li>
					
				<!-- Otro -->
					<li>
						<condition name ="Lugar">
						
							<!-- UVIGO-->
							
							<li value="UVIGO">Estoy en la Uvigo</li>
							
							<!-- Otro-->
							<li><think><set var="Solucion">false</set></think></li>	
						</condition>
	
					</li>
				</condition>
			</li>
		</condition>
		
		<!-- Shifteo (Se elimina la primera palabra y se vuelve a probar)-->
		
		<condition var ="Solucion">
			<li value="false">
				<condition var ="Fin">
					<li value="UnknownFinal">Soy un poco sensible.</li>
					<li><srai><star index= "2"/></srai></li>
				</condition>
			</li>
			<li></li>
		</condition>
		
		<!-- Reset-->
		<think>
			<set name="Lugar">unknown</set>
			<set name="Tema">unknown</set>
			<set name="Pregunta">unknown</set>
			<set name="verboEspecifico">unknown</set>
			<set name="TemaEspecifico">unknown</set>
			<set name="Resultado">unknown</set>
			<set name="Lista">unknown</set>
		</think>
	</template>
</category>

<category>
	<pattern>$GETLISTA *</pattern>
	<template>
		<think><set var="PrimerElem"><star/></set></think>
		<condition name ="Resultado">
			<li value="unknown"><think><set name="Resultado">0</set></think></li>
		</condition>

		<condition var ="PrimerElem">
			<li value="unknown"></li>
			<li>
				<think>
					<set name="Resultado"><map name="sucessor"><get name="Resultado"/></map></set>
					<condition name ="Lista">
						<li value="unknown"><set name="Lista"><get var="PrimerElem"/></set></li>
						<li><set name="Lista"><get name="Lista"/>, <get var="PrimerElem"/></set></li>
					</condition>
				</think>
				<srai>GETLISTA <map name="listar"><star/></map></srai>
			</li>
		</condition>
	</template>
</category>

